#!/usr/bin/python3

def safe_print_list_integers(my_lisy=[], x=0):
    count = 0
    for i in range(x):
        try:
            print("{:d}".format(my_lisy[i]), end="")
            count += 1
        except (ValueError, TypeError):
            pass
    print(end="\n")
    print("N = {}".format(count))
    return count
